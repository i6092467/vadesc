python ../main.py --data mnist --seed 1009 --no-survival --num_epochs 300
python ../main.py --data mnist --seed 1279 --no-survival --num_epochs 300
python ../main.py --data mnist --seed 1597 --no-survival --num_epochs 300
python ../main.py --data mnist --seed 1811 --no-survival --num_epochs 300
python ../main.py --data mnist --seed 1949 --no-survival --num_epochs 300
python ../main.py --data mnist --seed 2053 --no-survival --num_epochs 300
